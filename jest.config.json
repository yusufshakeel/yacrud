{
  "testEnvironment": "node",
  "verbose": true,
  "bail": true,
  "testRegex": "(unit).test.js$",
  "collectCoverage": true,
  "collectCoverageFrom": [
    "**/*.{js,jsx}",
    "!**/coverage/**",
    "!**/docs/**",
    "!**/node_modules/**",
    "!**/test/**",
    "!src/index.js",
    "!src/setups/**",
    "!src/configs/**",
    "!src/migrations/data/**",
    "!src/migrations/tables/**"
  ]
}